{
  "name": "predicate",
  "version": "1.2.0",
  "description": "A set of predicate functions to improve your value testing and comparisons.",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run -s clean && tsc",
    "watch": "tsc --watch",
    "lint": "eslint .",
    "test": "nyc -s mocha -R dot --recursive test",
    "posttest": "npm run -s view-cov && npm run -s check-cov && eslint .",
    "prepublishOnly": "npm run -s clean && tsc",
    "docs": "",
    "check-cov": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "html-cov": "nyc report --reporter=html",
    "view-cov": "nyc report",
    "browse-cov": "npm run-s html-cov && open coverage/index.html"
  },
  "main": "index.js",
  "author": {
    "name": "Trevor Landau",
    "email": "landautrevor@gmail.com",
    "url": "http://trevorlandau.net"
  },
  "homepage": "http://landau.github.io/predicate",
  "repository": {
    "type": "git",
    "url": "http://github.com/landau/predicate"
  },
  "bugs": {
    "url": "http://github.com/landau/predicate/issues"
  },
  "keywords": [
    "functional",
    "predicates",
    "predicate",
    "pred",
    "type",
    "instance",
    "comparator",
    "compare",
    "curry",
    "partial",
    "testing",
    "test",
    "chain",
    "chaining"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "./dist/**/*"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "devDependencies": {
    "@landau/eslint-config": "1.0.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsdoc": "4.8.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "lodash": "4.17.10",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "should": "13.2.1",
    "uglify-js": "2.4.19"
  }
}
